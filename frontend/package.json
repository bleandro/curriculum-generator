    "react",
    "webpack",
    "babel"
  ],
  "author": "Rafael Maia Chieregatto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafamaiach/curriculum-generator/issues"
  },
  "homepage": "https://github.com/rafamaiach/curriculum-generator#readme",
  "dependencies": {
