{
  "name": "curriculum-generator",
  "version": "1.0.0",
  "description": "Curriculum Generator using React and Apache POI (Java)",
  "main": "index.js",
  "private": false,
  "scripts": {
    "prod-start": "npm run build && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server/app.js",
    "dev-start": "cross-env NODE_ENV=development && nodemon server/app.js",
    "build": "cross-env NODE_ENV=production && webpack --config ./webpack.config.js --progress --colors",
    "dev-build": "cross-env NODE_ENV=development && webpack --config ./webpack.config.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafamaiach/curriculum-generator.git"
  },
